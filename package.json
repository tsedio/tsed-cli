{
  "name": "@tsed/cli-root",
  "version": "2.6.0",
  "description": "CLI to bootstrap your Ts.ED project",
  "private": true,
  "scripts": {
    "configure": "monorepo ci configure",
    "clean": "monorepo clean workspace",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn test:unit",
    "test:lint": "eslint packages --ext .ts",
    "test:lint:fix": "eslint packages --ext .ts --fix",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "monorepo build",
    "sync:packages": "monorepo sync packages",
    "docs:serve": "yarn api:build && yarn vuepress:serve",
    "docs:build": "yarn api:build && yarn vuepress:build",
    "docs:publish": "yarn docs:build && monorepo publish ghpages",
    "api:build": "lerna run build && tsdoc",
    "vuepress:build": "cross-env NODE_ENV=production vuepress build docs",
    "vuepress:serve": "vuepress dev docs",
    "prettier": "prettier '{packages,test}/**/*.ts' --write",
    "release": "semantic-release",
    "release:dryRun": "semantic-release --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TypedProject/tsed-cli.git"
  },
  "keywords": [
    "ts",
    "log",
    "debug",
    "typescript",
    "trace"
  ],
  "author": "Romain Lenzotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TypedProject/tsed-cli/issues"
  },
  "homepage": "https://github.com/TypedProject/tsed-cli",
  "dependencies": {
    "@samverschueren/stream-to-observable": "0.3.1",
    "@tsed/core": "6.10.3",
    "@tsed/di": "6.10.3",
    "@tsed/logger": "^5.5.4",
    "any-observable": "0.5.1",
    "axios": "0.21.0",
    "chalk": "4.1.0",
    "commander": "5.1.0",
    "consolidate": "0.16.0",
    "execa": "4.1.0",
    "figures": "3.2.0",
    "fs-extra": "9.0.1",
    "handlebars": "4.7.6",
    "handlebars-helpers": "0.10.0",
    "inquirer": "7.3.3",
    "inquirer-autocomplete-prompt": "1.3.0",
    "js-yaml": "3.14.0",
    "lerna": "3.22.1",
    "listr": "0.14.3",
    "module-alias": "2.2.2",
    "read-pkg-up": "7.0.1",
    "registry-url": "5.1.0",
    "rxjs": "6.6.3",
    "semver": "7.3.2",
    "split": "1.0.1",
    "tslib": "2.0.3",
    "update-notifier": "5.0.1"
  },
  "devDependencies": {
    "@tsed/markdown-it-symbols": "3.3.0",
    "@tsed/monorepo-utils": "1.11.0",
    "@typedproject/ts-doc": "4.0.12",
    "@types/jest": "26.0.0",
    "@types/node": "14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "concurrently": "5.1.0",
    "coveralls": "3.0.9",
    "cross-env": "7.0.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "24.0.1",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "gflow": "5.1.1",
    "husky": "4.2.3",
    "jest": "26.6.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "semantic-release": "^17.0.4",
    "sinon": "9.0.0",
    "sinon-chai": "3.5.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "tslint": "6.0.0",
    "typescript": "4.0.3",
    "tsconfig-paths": "3.9.0",
    "vue-analytics": "5.22.1",
    "vuepress": "1.7.1",
    "vuepress-theme-tsed": "3.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "directories": {
    "packages": "packages",
    "test": "test"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "monorepo": {
    "productionBranch": "master",
    "developBranch": "master",
    "npmAccess": "public",
    "ghpages": {
      "dir": "./docs/.vuepress/dist",
      "url": "https://github.com/TypedProject/tsed-cli.git",
      "branch": "gh-pages",
      "cname": "cli.tsed.io"
    }
  }
}

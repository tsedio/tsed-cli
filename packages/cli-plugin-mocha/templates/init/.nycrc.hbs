{
  "include": [
    "{{srcDir}}/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.d.ts",
    "**/index.ts",
    "**/interfaces/**",
    "**/*.spec.ts"
  ],
  "reporter": [
    "text-summary",
    "html",
    "lcov"
  ],
  "extension": [
    ".ts"
  ],
  "check-coverage": true,
  "lines": 70,
  "statements": 70,
  "functions": 70,
  "branches": 70,
  "all": true
}

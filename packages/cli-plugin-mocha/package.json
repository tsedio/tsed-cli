{
  "name": "@tsed/cli-plugin-mocha",
  "version": "5.1.0",
  "description": "Ts.ED CLI plugin. Add Mocha support",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn build:ts",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json && cp scripts/templateDir.esm.js lib/esm/utils/templateDir.js",
    "lint": "eslint '**/*.{ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 --passWithNoTests && jest-coverage-thresholds-bumper"
  },
  "devDependencies": {
    "@tsed/cli": "5.1.0",
    "@tsed/cli-core": "5.1.0",
    "@tsed/eslint": "5.1.0",
    "@tsed/jest-config": "5.1.0",
    "@tsed/typescript": "5.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.22.0",
    "jest": "29.5.0"
  },
  "dependencies": {
    "tslib": "2.3.1"
  },
  "peerDependencies": {}
}
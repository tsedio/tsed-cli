{
  "name": "@tsed/cli-testing",
  "version": "5.0.2",
  "description": "Utils to test you CLI based on Ts.ED CLI",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn build:ts",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json",
    "lint": "eslint '**/*.{ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 --passWithNoTests && jest-coverage-thresholds-bumper"
  },
  "keywords": [
    "Ts.ED",
    "cli",
    "typescript",
    "framework",
    "rest",
    "api",
    "express",
    "decorators"
  ],
  "dependencies": {
    "@tsed/cli-core": "5.0.2",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@tsed/eslint": "5.0.2",
    "@tsed/jest-config": "5.0.2",
    "@tsed/typescript": "5.0.2",
    "cross-env": "7.0.3",
    "eslint": "8.22.0",
    "jest": "29.5.0"
  }
}
